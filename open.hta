<html>
<head>
<script language="VBScript"> 
Dim selectedPath
Sub RunProgram(path)
    Set objShell = CreateObject("Wscript.Shell")
    objShell.Run path
End Sub
selectedPath = InputBox("Enter path of program to execute:")
RunProgram(selectedPath)
</script>
</head> 
<body>
    Nothing to see here..
</body>
</html>
